{
  "name": "storial",
  "version": "1.0.0",
  "description": "AI-powered component stories for React/Next.js - like Storybook, but magical",
  "type": "module",
  "bin": {
    "storial": "./bin/cli.js"
  },
  "main": "dist/server/index.js",
  "files": [
    "dist/",
    "bin/",
    "server/",
    "src/",
    "public/",
    "vite.config.ts",
    "tsconfig.json",
    "tsconfig.server.json",
    "tailwind.config.js",
    "postcss.config.js",
    "components.json",
    "README.md",
    "LICENSE"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/AmElmo/storial.git"
  },
  "homepage": "https://github.com/AmElmo/storial#readme",
  "bugs": {
    "url": "https://github.com/AmElmo/storial/issues"
  },
  "keywords": [
    "storybook",
    "react",
    "nextjs",
    "next.js",
    "components",
    "stories",
    "ai",
    "testing",
    "visual-testing",
    "developer-tools"
  ],
  "author": "AmElmo",
  "license": "MIT",
  "scripts": {
    "dev": "concurrently \"npm run dev:server\" \"npm run dev:ui\"",
    "dev:server": "tsx watch server/index.ts",
    "dev:ui": "vite",
    "build": "vite build && tsc -p tsconfig.server.json",
    "start": "node dist/server/index.js"
  },
  "dependencies": {
    "chalk": "^5.3.0",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.1.1",
    "cors": "^2.8.5",
    "dotenv": "^17.2.3",
    "express": "^4.19.2",
    "glob": "^10.3.10",
    "lucide-react": "^0.378.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-router-dom": "^6.23.1",
    "tailwind-merge": "^2.3.0",
    "typescript": "^5.4.5"
  },
  "devDependencies": {
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/node": "^20.12.7",
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.0",
    "@vitejs/plugin-react": "^4.3.0",
    "autoprefixer": "^10.4.19",
    "concurrently": "^8.2.2",
    "postcss": "^8.4.38",
    "tailwindcss": "^3.4.3",
    "tailwindcss-animate": "^1.0.7",
    "tsx": "^4.20.6",
    "vite": "^5.2.11"
  }
}
